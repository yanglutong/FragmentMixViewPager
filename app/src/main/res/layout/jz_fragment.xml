            android:layout_height="wrap_content"
            android:layout_below="@+id/bt_qiehuan"
            android:layout_alignParentRight="true"
            android:text="查询基站" />

        <Button
            android:id="@+id/bt_clear"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_below="@+id/bt_jizhan"
            android:layout_alignParentRight="true"
            android:text="清空地图" />

        <Button
            android:id="@+id/bt_location"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_below="@+id/bt_clear"
            android:layout_alignParentRight="true"
            android:text="当前位置" />

        <Button
            android:id="@+id/bt_jt"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_below="@+id/bt_location"
            android:layout_alignParentRight="true"
            android:text="交通地图" />

        <Button
            android:id="@+id/bt_hot"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_below="@+id/bt_jt"
            android:layout_alignParentRight="true"
            android:text="热力地图" />

        <Button
            android:id="@+id/bt_juli"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_below="@+id/bt_hot"
            android:layout_alignParentRight="true"
            android:text="距离测量" />

        <Button
            android:id="@+id/bt_jingbao"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_below="@+id/bt_juli"
            android:layout_alignParentRight="true"
            android:text="位置警报" />

        <!--<com.baidu.mapapi.map.MapView-->
        <!--android:id="@+id/bmapView"-->
        <!--android:layout_width="match_parent"-->
        <!--android:layout_height="wrap_content"-->
        <!--android:clickable="true" />-->
    </RelativeLayout>
    <LinearLayout
        android:id="@+id/liner"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_alignParentBottom="true"
        android:orientation="vertical"
        android:visibility="visible">

        <LinearLayout
            android:id="@+id/lines"
            android:visibility="gone"
            android:layout_gravity="right"
            android:background="@drawable/bg_shape_jz_fragment"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content">
            <ImageView
                android:layout_gravity="center"
                android:id="@+id/iv_up"
                android:layout_width="@dimen/navi_dimens_40dp"
                android:layout_height="@dimen/navi_dimens_20dp"
                android:src="@mipmap/up"></ImageView>
        </LinearLayout>
        <LinearLayout
            android:visibility="visible"
            android:id="@+id/lab"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:background="@drawable/bg_shape_jz_fragment"
            android:orientation="vertical">
            <LinearLayout
                android:layout_marginRight="10dp"
                android:layout_gravity="right"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content">
                <ImageView
                    android:id="@+id/iv_down"
                    android:layout_width="@dimen/navi_dimens_30dp"
                    android:layout_height="@dimen/navi_dimens_20dp"
                    android:layout_gravity="right"
                    android:src="@mipmap/down"></ImageView>
            </LinearLayout>
            <RadioGroup
                android:layout_width="match_parent"
                android:layout_height="@dimen/x30"
                android:layout_marginLeft="15dp"
                android:layout_marginRight="10dp"
                android:background="@color/white"
                android:orientation="horizontal">

                <CheckBox
                    android:id="@+id/rb_yidong"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:layout_gravity="center"
                    android:layout_weight="1"
                    android:checked="true"
                    android:text="移动"
                    android:textSize="12dp"
                    android:theme="@style/MyCheckBox" />

                <CheckBox
                    android:id="@+id/rb_liantong"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:layout_gravity="center"
                    android:layout_weight="1"
                    android:text="联通"
                    android:textSize="12dp"
                    android:theme="@style/MyCheckBox" />

                <CheckBox
                    android:id="@+id/rb_ldainxin4"
                    android:layout_width="match_parent"
                    android:layout_height="match_parent"
                    android:layout_gravity="center"
                    android:layout_weight="1"
                    android:text="电信"
                    android:textSize="12dp"
                    android:theme="@style/MyCheckBox" />

                <CheckBox
                    android:id="@+id/rb_gd"
                    android:layout_width="match_parent"
                    android:layout_height="match_parent"
                    android:layout_gravity="center"
                    android:layout_weight="1"
                    android:text="广电"
                    android:textSize="12dp"
                    android:theme="@style/MyCheckBox" />

                <CheckBox
                    android:id="@+id/rb_cdma"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:layout_gravity="center"
                    android:layout_weight="0.9"
                    android:text="电信CDMA"
                    android:textSize="12dp"
                    android:theme="@style/MyCheckBox" />
            </RadioGroup>

            <View
                android:id="@+id/view_check"
                android:layout_width="match_parent"
                android:layout_height="1dp"
                android:background="#E3E4E6"
                android:visibility="gone" />

            <LinearLayout
                android:id="@+id/ll_sid"
                android:layout_width="match_parent"
                android:layout_height="@dimen/x30"
                android:background="@color/white"
                android:orientation="horizontal"
                android:visibility="gone">

                <TextView
                    android:layout_width="130dp"
                    android:layout_height="match_parent"
                    android:layout_marginLeft="20dp"
                    android:gravity="center_vertical"
                    android:text="SID:"
                    android:textColor="@color/gray_black" />

                <EditText
                    android:id="@+id/et_sid"
                    android:layout_width="match_parent"
                    android:layout_height="match_parent"
                    android:layout_marginRight="10dp"
                    android:background="@null"
                    android:hint="CDMA请输入SID"
                    android:inputType="number"
                    android:text=""
                    android:textSize="12dp" />
            </LinearLayout>


            <LinearLayout
                android:id="@+id/ll_nid"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:background="@color/white"
                android:orientation="horizontal"
                android:visibility="gone">

                <TextView
                    android:layout_width="130dp"
                    android:layout_height="wrap_content"
                    android:layout_marginLeft="20dp"
                    android:text="请输入lac:"
                    android:textColor="@color/gray_black" />

                <EditText
                    android:id="@+id/et_nid"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:layout_marginRight="10dp"
                    android:hint="请输入nid"
                    android:inputType="number"
                    android:text="" />
            </LinearLayout>

            <LinearLayout
                android:id="@+id/ll_bid"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:background="@color/white"
                android:orientation="horizontal"
                android:visibility="gone">

                <TextView
                    android:layout_width="130dp"
                    android:layout_height="wrap_content"
                    android:layout_marginLeft="20dp"
                    android:text="请输入ci:"
                    android:textColor="@color/gray_black" />

                <EditText
                    android:id="@+id/et_bid"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:layout_marginRight="10dp"
                    android:hint="请输入bid"
                    android:inputType="number"
                    android:text="" />
            </LinearLayout>

            <View
                android:layout_width="match_parent"
                android:layout_height="1dp"
                android:background="#E3E4E6" />

            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="@dimen/x30"
                android:background="@color/white"
                android:orientation="horizontal">

                <TextView
                    android:layout_width="130dp"
                    android:layout_height="wrap_content"
                    android:layout_marginLeft="20dp"
                    android:text="TAC/LAC/NID:"
                    android:textColor="@color/gray_black"
                    android:textSize="12dp" />

                <EditText
                    android:id="@+id/et_taclac"
                    android:layout_weight="1"
                    android:layout_width="match_parent"
                    android:layout_height="match_parent"
                    android:layout_marginRight="10dp"
                    android:background="@null"
                    android:hint="请输入TAC/LAC/NID"
                    android:inputType="number"
                    android:text="12562"
                    android:textSize="12dp" />
            </LinearLayout>

            <View
                android:layout_width="match_parent"
                android:layout_height="1dp"
                android:background="#E3E4E6" />


            <LinearLayout

                android:layout_width="match_parent"
                android:layout_height="@dimen/x30"
                android:background="@color/white"
                android:orientation="horizontal">

                <TextView
                    android:layout_width="130dp"
                    android:layout_height="wrap_content"
                    android:layout_gravity="center"
                    android:layout_marginLeft="20dp"
                    android:text="ECI/CI/BID:"
                    android:textColor="@color/gray_black"
                    android:textSize="12dp" />

                <EditText
                    android:id="@+id/et_eci"
                    android:layout_width="match_parent"
                    android:layout_height="match_parent"
                    android:layout_marginRight="10dp"
                    android:background="@null"
                    android:hint="请输入ECI/CI/BID"
                    android:inputType="number"
                    android:text="46026774"
                    android:textSize="12dp" />
            </LinearLayout>

            <!--        <View-->
            <!--            android:layout_width="match_parent"-->
            <!--            android:layout_height="1dp"-->
            <!--            android:background="#E3E4E6" />-->

            <LinearLayout
                android:id="@+id/ll_location"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:background="@color/white"
                android:orientation="horizontal"
                android:visibility="gone"
                android:weightSum="2">

                <LinearLayout
                    android:layout_width="0dp"
                    android:layout_height="wrap_content"
                    android:layout_weight="1">

                    <TextView
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_gravity="center"
                        android:layout_marginLeft="20dp"
                        android:text="经度（WGS）:"
                        android:textColor="@color/gray_black"
                        android:textSize="12dp" />

                    <EditText
                        android:id="@+id/ets_lon"
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:layout_marginRight="10dp"
                        android:hint="请输入经度"
                        android:inputType="numberDecimal"
                        android:text=""
                        android:textSize="12dp" />
                </LinearLayout>

                <LinearLayout
                    android:layout_width="0dp"
                    android:layout_height="wrap_content"
                    android:layout_weight="1">

                    <TextView
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_gravity="center"
                        android:layout_marginLeft="20dp"
                        android:text="纬度（WGS）:"
                        android:textColor="@color/gray_black"
                        android:textSize="12dp" />

                    <EditText
                        android:id="@+id/ets_lat"
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:layout_marginRight="10dp"
                        android:hint="请输入纬度"
                        android:inputType="numberDecimal"
                        android:text=""
                        android:textSize="12dp" />
                </LinearLayout>
            </LinearLayout>

            <View
                android:layout_width="match_parent"
                android:layout_height="1dp"
                android:background="#E3E4E6" />

            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:background="@color/white"
                android:orientation="horizontal">

                <TextView
                    android:layout_width="130dp"
                    android:layout_height="wrap_content"
                    android:layout_gravity="center"
                    android:layout_marginLeft="20dp"
                    android:text="基站覆盖半径(米):"
                    android:textColor="@color/gray_black"
                    android:textSize="12dp" />

                <LinearLayout
                    android:layout_width="match_parent"
                    android:layout_height="@dimen/x30"
                    android:layout_marginRight="10dp"
                    android:layout_weight="2"
                    android:orientation="horizontal">

                    <EditText
                        android:id="@+id/et_ta"
                        android:layout_width="match_parent"
                        android:layout_height="match_parent"
                        android:layout_marginRight="10dp"
                        android:layout_weight="2"
                        android:background="@null"
                        android:hint="请输入半径值"
                        android:inputType="number|numberDecimal"
                        android:text=""
                        android:textSize="12dp" />

                    <Button
                        android:id="@+id/btadd"
                        android:layout_width="0dp"
                        android:layout_height="wrap_content"
                        android:layout_weight="2"
                        android:background="#2f8dff"
                        android:text="添加TA值"
                        android:textColor="#FFFFFF"
                        android:textSize="17dp"
                        android:visibility="gone" />

                    <LinearLayout
                        android:layout_width="match_parent"
                        android:layout_height="match_parent"
                        android:layout_weight="5"
                        android:gravity="center">

                        <Button
                            android:id="@+id/bt_adddilao"
                            android:layout_width="wrap_content"
                            android:layout_height="32dp"
                            android:background="@drawable/shape_button_queren"
                            android:text="查询"
                            android:textColor="#FFFFFF"
                            android:textSize="17dp" />
                    </LinearLayout>
                </LinearLayout>
            </LinearLayout>

            <!--        <View-->
            <!--            android:layout_width="match_parent"-->
            <!--            android:layout_height="1dp"-->
            <!--            android:background="@color/color_7f000000" />-->

        </LinearLayout>
    </LinearLayout>

